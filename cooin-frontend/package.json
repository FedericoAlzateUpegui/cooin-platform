{
  "name": "cooin-frontend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "EXPO_NO_DOTENV=1 expo start --web --port 8083",
    "web:fast": "EXPO_NO_DOTENV=1 expo start --web --port 8083 --max-workers 2",
    "web:dev": "expo start --web --port 8083 --dev-client",
    "build:web": "expo export --platform web",
    "deploy": "vercel --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "clean": "rm -rf .expo node_modules/.cache",
    "clean:full": "rm -rf .expo node_modules && npm install"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-picker/picker": "^2.11.4",
    "@react-navigation/bottom-tabs": "^7.8.5",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/stack": "^7.6.4",
    "axios": "^1.7.9",
    "expo": "^54.0.23",
    "expo-document-picker": "^14.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-localization": "^17.0.7",
    "expo-secure-store": "^15.0.7",
    "expo-status-bar": "~3.0.8",
    "i18next": "^25.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.2",
    "react-i18next": "^16.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-web": "^0.21.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "typescript": "~5.9.2"
  },
  "private": true
}
